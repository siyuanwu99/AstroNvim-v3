    "Create 2D Plot environment": {
        "prefix": [
            "plotenvironment2d",
            "\\plotenvironment2d"
        ],
        "body": [
            "\\begin{tikzpicture}",
            "\\begin{axis}[",
            "legend pos=outer north east,",
            "title=${1:Example},",
            "axis lines =${2| box, left, middle, center, right, none|},",
